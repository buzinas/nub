{
  "name": "nub",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node ./server.mjs",
    "build": "vite build && vite build --ssr",
    "start": "NODE_ENV=production node ./server.mjs",
    "migration:create": "drizzle-kit generate:sqlite",
    "migration:drop": "drizzle-kit drop",
    "migration:run": "esrun ./app/db/migrate.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@digitak/esrun": "^3.2.25",
    "@paralleldrive/cuid2": "^2.2.2",
    "@remix-run/express": "^2.3.0",
    "@remix-run/node": "^2.3.0",
    "@remix-run/react": "^2.3.0",
    "better-sqlite3": "^9.1.1",
    "directory-tree": "^3.5.1",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-aria": "^3.30.0",
    "react-aria-components": "1.0.0-rc.0",
    "react-dom": "^18.2.0",
    "requestidlecallback": "^0.3.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "@remix-run/dev": "^2.3.0",
    "@remix-run/eslint-config": "^2.3.0",
    "@types/better-sqlite3": "^7.6.7",
    "@types/express": "^4.17.20",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/typescript-estree": "^6.11.0",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.54.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
